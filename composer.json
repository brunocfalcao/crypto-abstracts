{
    "name": "brunocfalcao/crypto-abstracts",
    "description": "Nidavellir crypto abstracts",
    "keywords": ["Crypto"],
    "homepage": "https://github.com/brunocfalcao/crypto-abstracts",
    "license": "MIT",
    "authors": [{
        "name": "Bruno Falcao",
        "email": "bruno.c.falcao@brunofalcao.me",
        "homepage": "https://www.brunocfalcao.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\CryptoAbstracts\\": "src",
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\CryptoAbstracts\\CryptoAbstractsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
